interface "GET /self-employers/{vatin}/orders" as getSelfEmplsIdOrders
interface "GET /orders/{id}" as getOrdersId

Information --() getSelfEmpls
Information --() getSelfEmplsId
Information --() getOrdersId
Information --() getSelfEmplsIdOrders
Information --() getSelfEmplsIdPermission
Information --() getRestrictions
Information --() postVatinRequest

Information -up-( getTaxpayerRestrictions
Information -up-( getTaxpayerStatus
Information -up-( getInnByPersonalInfo
Information -up-( getGrantedPermission


Component "Modification self-employer" as Modification <<component>>
interface "PUT /self-employers/{vatin}" as putSelfEmplsId
interface "DELETE /self-employers/{vatin}" as delSelfEmplsId

Modification --() putSelfEmplsId
Modification --() delSelfEmplsId

Modification -up-( putTaxpayerData
Modification -up-( postPartnerUnbind
Modification -up-( postTaxpayerUnregistration

}

@enduml



